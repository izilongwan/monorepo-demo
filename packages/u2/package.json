{
  "name": "@monorepo-demo/u2",
  "version": "1.0.0",
  "description": "Math utility library - minus function",
  "type": "module",
  "main": "dist/u2.cjs",
  "module": "dist/u2.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/u2.es.js",
      "require": "./dist/u2.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "vite:build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "build": "tsc --target ES2020 --declaration --outDir dist",
    "dev": "node --loader ts-node/esm index.ts"
  },
  "keywords": [
    "math",
    "utils"
  ],
